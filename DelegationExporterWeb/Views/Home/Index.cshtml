@{
    ViewData["Title"] = "Home Page";
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
</head>

<body>
    <form method="post" enctype="multipart/form-data" action="/home/UploadXls">
        <input type="file" name="files" multiple accept=".xls, .xlsx" />
        <p>
        <p>
            <input type="submit" value="UploadXls" />
    </form>
    <p>
        <div id="test_button">
            <button v-on:click="test" type="button" class="btn btn-success">測試</button>
        </div>

        <script>
            new Vue({
                el: '#test_button',
                data:
                {
                    info: null
                },
                methods:
                {
                    test: function () {

                        console.log('click');
                        axios
                            .post(window.location.origin + "/home/test")
                            .then(response => {
                                this.info = response.data.value;
                                console.log(this.info);
                            })
                            .catch(function (error) { 
                                console.log('NO');
                            });
                    }
                }
            })

        </script>
</body>
</html>
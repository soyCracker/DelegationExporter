@{
    ViewData["Title"] = "Table Page";
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
</head>

<body>
    <div id="man_table">
        <button v-on:click="click_button" type="button" class="btn btn-success">Click</button>

        <p> {{ test }} </p>

        <div>
            <table class="table">
                <tbody>
                    <tr v-for="(item, index) in list" :key="index">
                        <td>{{ item.name }}</td>
                        <td>{{ item.age }}</td>
                        <td>{{ item.money }}</td>
                    </tr>
                </tbody>
            </table>
            <ul>
                <li v-for="(item, index) in member" :key="index">{{ item }}</li>
            </ul>
        </div>
    </div>
    

    <script>
        console.log("123456");

        var table_app = new Vue({
            el: '#man_table',
            data:
            {

                test: '777777',
                member: ['Adam', 'Jack', 'Candy', 'Louis', 'Lurry'],
                list: [{ name: 'a', money: 1, age:12},
                    { name: 'b', money: 5, age: 25 }
                ],
                
            },
            methods:
            {

                click_button: function ()
                {
                    var self = this;
                    console.log(this.test);
                    self.test='123456798'
                    
                    axios.post('/api/File/GetData')
                        .then(function (response) {
                            self.list = response.data.response;
                            console.log('get');
                            console.log(response.data.response[0].name);
                            self.test = response.data.response[0].name;
                            console.log(self.test);

                        })
                        .catch(function (error) {
                            console.log(error);
                        });
                },

            }
        })
        
    </script>
</body>
</html>